        <div class="inner_breadcrumb">
            <ul class="short">
                <li>
                    @Html.ActionLink("TRANG CHỦ", "Index", "Home")
                    <i>|</i>
                </li>
                <li> DANH DÁCH SẢN PHẨM </li>
                @if (!string.IsNullOrEmpty(ViewBag.CategoryName))
                {
                    <li>
                        <i>|</i>
                        @ViewBag.CategoryName
                    </li>
                }
            </ul>
        </div>
    </div>
</div>
